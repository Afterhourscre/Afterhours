<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Aheadworks\Faq\Block\Search\Search" name="aw_faq.search_title" template="Aheadworks_Faq::title.phtml"/>
            <block class="Aheadworks\Faq\Block\Search\Search" name="aw_faq.search" template="Aheadworks_Faq::search/search_field.phtml" after="aw_faq.search_title"/>
            <block class="Aheadworks\Faq\Block\Search\Search" name="aw_faq.search_results" template="Aheadworks_Faq::search/search_results.phtml"/>
        </referenceContainer>
    </body>
</page>
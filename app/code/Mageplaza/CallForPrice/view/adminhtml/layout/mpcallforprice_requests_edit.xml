<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="editor"/>
    <body>
        <referenceContainer name="content">
            <block class="Mageplaza\CallForPrice\Block\Adminhtml\Requests\Edit" name="callforprice_requests_edit"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Mageplaza\CallForPrice\Block\Adminhtml\Requests\Edit\Tabs"
                   name="callforprice_requests_edit_tabs">
                <block class="Mageplaza\CallForPrice\Block\Adminhtml\Requests\Edit\Tab\General"
                       name="callforprice_requests_edit_tab_general"/>
                <action method="addTab">
                    <argument name="name" xsi:type="string">callforprice</argument>
                    <argument name="block" xsi:type="string">callforprice_requests_edit_tab_general</argument>
                </action>
            </block>
        </referenceContainer>
    </body>
</page>

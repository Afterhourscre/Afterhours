<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <chatsystem>
            <chat_config>
                <chat_name>Webkul Support</chat_name>
                <host_name>example.com</host_name>
                <port_number>8080</port_number>
                <assign_automatic>1</assign_automatic>
                <offline_message>0</offline_message>
                <show_rating_box_time>120</show_rating_box_time>
            </chat_config>
        </chatsystem>
    </default>
</config>

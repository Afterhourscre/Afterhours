<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Aheadworks\Faq\Block\Category\Category" name="aw_faq.category_name" template="Aheadworks_Faq::title.phtml" />
            <block class="Aheadworks\Faq\Block\Search\Search" ifconfig="faq/general/faq_search_enabled" name="aw_faq.search" template="Aheadworks_Faq::search/search_field.phtml" after="aw_faq.category_name"/>
            <block class="Aheadworks\Faq\Block\Category\Category" name="aw_faq.category" template="Aheadworks_Faq::category.phtml" />
        </referenceContainer>
    </body>
</page>